{
  "name": "api",
  "version": "3.0.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "dev": "tsup --watch --onSuccess \"phase run --path /api node dist/index.js\"",
    "build": "tsup",
    "start": "node dist/index.js",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "create-modal-snapshot": "phase run --path /api \"export MODAL_IMAGE_BUILDER_VERSION=2025.06 && tsx scripts/create-modal-snapshot.ts\"",
    "test": "phase run --path /api vitest run",
    "test:watch": "phase run --path /api vitest"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.53",
    "@anthropic-ai/sdk": "^0.54.0",
    "@aws-sdk/client-s3": "3.928.0",
    "@aws-sdk/s3-request-presigner": "^3.928.0",
    "@daytonaio/sdk": "^0.108.0",
    "@libsql/client": "^0.15.15",
    "@modelcontextprotocol/sdk": "^1.25.1",
    "@openrouter/ai-sdk-provider": "^1.2.5",
    "@posthog/ai": "^6.4.3",
    "@prisma/client": "6.17.1",
    "@shipper/convex": "workspace:*",
    "@shipper/database": "workspace:*",
    "@shipper/shared": "workspace:*",
    "@stripe/agent-toolkit": "^0.7.12",
    "@trpc/server": "^11.4.2",
    "@voltagent/core": "^1.1.19",
    "ai": "^5.0.112",
    "cors": "^2.8.5",
    "dotenv": "^17.0.1",
    "express": "^4.19.2",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.3",
    "mammoth": "^1.11.0",
    "modal": "^0.5.0",
    "pino": "^10.1.0",
    "posthog-node": "^5.9.2",
    "resumable-stream": "^2.2.8",
    "stripe": "^18.3.0",
    "throttleit": "^2.1.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^4.0.7",
    "eslint": "^9.34.0",
    "pino-pretty": "^13.1.2",
    "tsup": "^8.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.9.2",
    "vitest": "^4.0.7"
  }
}
